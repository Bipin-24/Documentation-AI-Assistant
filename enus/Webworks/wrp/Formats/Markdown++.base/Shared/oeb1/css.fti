    </Option>
   </Options>

   <Properties>
    <!-- HTML -->
    <!--      -->
    <Property name="tag" group="html" default="table">
     <PropertyClass name="html-element" />
    </Property>
    <Property name="float" group="html" default="none">
     <PropertyClass name="float" />
    </Property>
    <Property name="clear" group="html" default="none">

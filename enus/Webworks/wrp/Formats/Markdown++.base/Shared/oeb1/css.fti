     <PropertyClass name="auto" />
     <PropertyClass name="units" />
    </Property>
    <Property name="margin-right" group="margin" default="auto">
     <PropertyClass name="auto" />
     <PropertyClass name="units" />
    </Property>
    <Property name="margin-top" group="margin" default="auto">
     <PropertyClass name="auto" />
     <PropertyClass name="units" />
    </Property>
    <Property name="margin-bottom" group="margin" default="auto">
     <PropertyClass name="auto" />
     <PropertyClass name="units" />
    </Property>

    <!-- Spacing -->
    <!--         -->
    <Property name="cell-padding" group="table" default="0">
     <PropertyClass name="number" />
    </Property>
    <Property name="cell-spacing" group="table" default="0">
     <PropertyClass name="number" />
    </Property>

    <!-- Table -->
    <!--       -->
    <Property name="border" group="table" default="0">
     <PropertyClass name="number" />
    </Property>

    <!-- Vertical Alignment -->
    <!--                    -->
    <Property name="vertical-align" group="table" default="top">
     <PropertyClass name="cell-align-vertical" />
    </Property>

    <!-- Table Head Background -->
    <!--                       -->
    <Property name="thead-background-color" group="thead-background" default="transparent">
     <PropertyClass name="color-name" />
